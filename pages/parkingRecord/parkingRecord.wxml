<!--pages/parkingRecord/parkingRecord.wxml-->
<view>
    <view class='record-row' wx:for="{{list}}">
        <view class='num'>
            <text>流水号：{{item.sn}}</text>
            <view class='money'>￥<text>{{item.amout}}</text></view>
        </view>
        <view class='msg'>
            <view>使用人：{{item.phone}}</view>
            <view class='address'>
                <image src='../images/adrico.png'></image>
                <text>{{item.address}}</text>
            </view> 
            <view class='time'>
                <view class='lf'>{{item.startTime}}~{{item.endTime}}</view>
                <text class='rg'>{{item.time}}分</text>
            </view>
        </view>
    </view> 

</view>
