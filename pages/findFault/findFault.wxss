/* pages/findFault/findFault.wxss */
page{background-color:#fff;}
.main{padding:0 10px 55px 10px;font-size:16px;}
.otlTop{background-color:#efefef;padding:10px 10px 10px 50px;margin:10px 0;border-radius:3px;height:30px;line-height:30px;position:relative;}
.otlTop .ico{width:30px;position:absolute;top:11px;left:10px;}
.otlTop .otlinput{font-size:14px;color:#808080;height:30px;line-height:30px;width:100%;}
.otlTop .otlnum{font-size:14px;line-height:17px;}
.otlTop .otlnum .rescan{color:#f05b48;font-size:12px;}
.fault{padding:10px 0;}
.fault .glist{overflow:hidden;}
.fault .glist view{width:50%;float:left;height:40px;line-height:40px;overflow:hidden;}
.fault .glist .checkbox{width:20px;float:left;margin:10px 10px 0 0;}
.fault .glist .txt{display:block;float:left;}
.wchange{padding:15px 10px;background-color:#fff;}
.wchange .ali{height:40px;line-height:40px;border-radius:3px;background-color:#efefef;margin:10px 0;}
.wchange .ali>input{height:40px;line-height:40px;padding:0 10px;font-size:14px;}
.wchange .wec .add{width:60px;display:inline-block;vertical-align:middle;}
.wchange .wec .tips{margin-left:4%;display:inline-block;vertical-align:middle;font-size:12px;color:#808080;line-height:20px;}
.retext{font-size:14px;color:#808080;}
.retext .inbox{background-color:#efefef;padding:15px;border-radius:3px;}
.retext .inbox textarea{width:100%;}
.retext .retips{text-align:right;font-size:12px;line-height:25px;}
.commit{width:100%;height:45px;line-height:45px;text-align:center;position:fixed;bottom:0;left:0;color:#fff;background-color:#f05b48;}