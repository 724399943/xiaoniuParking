/**index.wxss**/
.map-box {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.map-header{ height: 40px; line-height: 40px; background:#191d21; }
.map-header .btn{ text-align: center; line-height: 40px; color: #fff; font-size: 14px; display: block; width: 50%; float: left;}
.mapclass{ width: 100%; height: 100%; position: relative; z-index: 1;}
.section-search{ position: absolute; width: 90%; z-index: 2; left: 50%; top: 8%; margin-left: -45%; background:#fff; height: 44px; line-height: 44px;  box-shadow: 0 0 10px #333; border-radius: 4px; }
.section-search .txt{ height: 44px; line-height:44px; font-size: 14px;margin-left: 40px; margin-right: 98px; color: #666; }
.section-search .search-box{ position: absolute; top: 11px; left:8px; width: 20px; height: 20px;}
.section-select{ position: absolute; top: 5px; right: 5px;z-index: 22; width: 90px; height: 34px;line-height: 34px; background: #f05b48; color: #fff; font-size: 14px; text-align: center;border-radius: 4px;}
.section-select .ico{ display: inline-block; width: 12px; height: 12px;vertical-align: middle; line-height: 10px; margin-left: 5px; }
.section-select .picker{ vertical-align: middle; display: inline-block; }
.section-select .t{ display: inline-block;vertical-align:middle;height:34px;line-height:34px; }
.user-box{ position: absolute; width: 40px;   bottom: 12%; left: 4%; z-index: 22; }
.ding-box{ position: absolute; width: 40px;   bottom: 4%; left: 4%; z-index: 22; }
.p-box{ position: absolute; width: 40px;   bottom: 12%; right: 4%; z-index: 22; }
.kefu-box{ position: absolute; width: 40px;   bottom: 4%; right: 4%; z-index: 22; }
.user-box .img,.ding-box .img,.kefu-box .img ,.p-box .img{ height: 40px; }
.sao-box{ height: 40px; width:140px; border-radius: 20px;  background: #000; position: absolute; bottom: 2%; left: 50%; margin-left: -70px; color: #fff; font-size: 16px; z-index: 22; text-align: center; line-height: 38px;}
.sao-box .img{ width: 18px;height:18px; display: inline-block; margin-right: 10px; vertical-align: middle; }
.sao-box .sao{height:40px;line-height:40px;display: inline-block;vertical-align:middle;}
.pop-msg-box{ display: block; position: absolute; width: 90%; left: 50%; top: 17%; margin-left: -45%; background: #fff; z-index: 2; box-shadow: 0 0 10px #333;  }
.pop-msg-box .tit{ height: 40px; padding: 0 15px 0 35px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; position: relative; line-height: 40px; font-size: 14px; }
.pop-msg-box .tit .ico{ width: 16px; height: 16px; position: absolute; top: 12px; left: 14px; }
.pop-main{ text-align: center}
.pop-main .item{ display: inline-block; padding:20px 10px 0 10px; font-size: 14px; color: #191d21; font-weight: bold; width: 110px; text-align: center }
.pop-main .item .num{ color: #f05b48; display: inline; font-size: 16px;overflow: hidden}
.pop-main .item .t{ display: inline; }
.pop-main .item .txt{ color: #999; padding-top: 6px; font-weight: 400; text-align: center; }
.pop-ps{ text-align: center; display: block; font-size: 16px; color: #333; padding: 10px 0;}
.pop-compant{ text-align: center; display: block; font-size: 14px; color: #999; padding-bottom: 10px;}
.pop-btn{ text-align: center; overflow: hidden; padding-bottom: 10px; margin-top: 10px;}
.pop-btn .border{ margin-bottom: 10px; }
.pop-btn .btn{ display: inline-block; border-radius: 4px; background: #f05b48; margin: 0 20px; height: 40px; line-height: 40px; width: 30%; text-align: center; color: #fff; font-size: 14px; }
.bb{ color: #f69;}
.pop-main-t{ position: absolute; width: 70%; left: 50%; margin-left: -35%; top: 20%; background: #fff;  border-radius: 10px; padding-top: 10%;}
.pop-main-t .tit{ color: #666; font-size: 16px; text-align: center; display: block; line-height: 24px; }
.pop-main-t .tit .num{color: #f05b48}
.pop-main-t .btn-box{ margin-top: 20px; height: 44px;}
.pop-main-t .btn-box .btn{ position: relative; height: 100%; text-align: center; display: block; width: 50%; float: left; font-size: 16px; line-height: 44px;}
.pop-btn .btn{ display: inline-block; border-radius: 4px; background: #f05b48; margin: 0 20px; height: 40px; line-height: 40px; width: 30%; text-align: center; color: #fff; font-size: 14px; }
.pop-main-t .tips{color:#808080;font-size:12px; margin-top:5px;}
.pop-main-t .tips .txt{display:inline-block;vertical-align:middle;}
.pop-main-t .tips .check{width:20px;height:20px;display:inline-block;vertical-align:middle;margin-left:8px;}
.popMask{width:100%;height:100%;position:fixed;top:0;left:0;background:rgba(0,0,0,.5);z-index:99;display:none;}
.popup{width:70%;background-color:#fff;font-size:14px;border-radius:5px;position:fixed;top:50%;left:50%;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-o-transform:translate(-50%,-50%);}
.popup .pcont{padding:10px 0;text-align:center;}
.popup .pcont .tit{color:#f05b48;padding:0 5px;display:inline-block;vertical-align:middle;}
.popup .pcont .pct{line-height:25px;}
.popup .pcont .tips{color:#808080;font-size:12px;}
.popup .pcont .tips .txt{display:inline-block;vertical-align:middle;}
.popup .pcont .tips .check{width:20px;height:20px;display:inline-block;vertical-align:middle;margin-left:5px;}
.pbtn{border-top:1px solid #eee;display:-webkit-box;border-bottom-left-radius:5px;border-bottom-right-radius:5px;}
.pbtn .btn{width:50%;height:45px;line-height:45px;text-align:center;}
.pbtn .btn.ad{color:#f05b48;margin-right:-1px;border-left:1px solid #eee;}
.close-pop-mb{ position: absolute; width: 100%; height: 100%; z-index: 1; top: 0; left: 0;}
.comein{ color: #f05b48 }
.pop-main-t .btn-box.ordering .comein{ width: 100%; }
